@model MeshApp.Pages.Nodes.IndexModel

@{
    ViewData["Title"] = "Monitor";
}

<h1>Nodes</h1>

<table class="table table-striped">
    @foreach (var item in Model.Nodes)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.NodeId</td>
            <td>@item.Temperature</td>
            <td>@item.Humidity</td>
            <td>@item.LastUpdate</td>
        </tr>
    }
</table>